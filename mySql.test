CREATE TABLE orders(
  id INT(11) AUTO_INCREMENT PRIMARY KEY,
  costomer_id INT(11) NOT NULL,
  amount INT(11) NOT NULL,
    status VARCHAR(255) NOT NULL,
    FOREIGN KEY (costomer_id) REFERENCES `custmores` (id)
);

